{
    "name": "btamilio/hs-dummy-json",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Btamilio\\HsDummyJson\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Btamilio\\HsDummyJson\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "authors": [
        {
            "name": "Brenden Tamilio",
            "email": "btamilio@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Btamilio\\HsDummyJson\\HsDummyJsonServiceProvider"
            ]
        }
    },
    "require": {
         "php": ">=8.1",
        "laravel/framework": "^11.9",
        "spatie/array-to-xml": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
